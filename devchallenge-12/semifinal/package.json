{
  "name": "gulp4",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "gulp"
  },
  "config": {
    "src": {
      "pug": {
        "main": "src/pug/pages/*.pug",
        "root": "src/pug",
        "modules": "src/pug/modules",
        "pages": "src/pug/pages",
        "all": "src/pug/**/*.pug"
      },
      "sass": {
        "main": "src/sass/*.sass",
        "root": "src/sass",
        "modules": "src/sass/modules",
        "pages": "src/sass/pages",
        "all": "src/sass/**/*.sass"
      },
      "js": {
        "main": "src/js/*.js",
        "root": "src/js",
        "modules": "src/js/modules",
        "all": "src/js/**/*.js"
      },
      "img": {
        "root": "src/img",
        "allImg": "src/img/*.*",
        "png": "src/img/png/*.*",
        "svg": "src/img/svg/*.*"
      },
      "fonts": {
        "root": "src/fonts",
        "pre": "src/fonts/*.ttf",
        "post": "src/fonts/*.{svg,eot,ttf,woff,woff2}"
      }
    },
    "dist": {
      "root": "dist",
      "css": "dist/css",
      "js": "dist/js",
      "img": "dist/img",
      "fonts": "dist/fonts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "browser-sync": "^2.24.4",
    "gulp": "gulpjs/gulp.git#4.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cache": "^1.0.2",
    "gulp-cheerio": "^0.6.3",
    "gulp-csscomb": "^3.0.8",
    "gulp-csso": "^3.0.1",
    "gulp-environments": "^0.1.2",
    "gulp-fontmin": "^0.7.4",
    "gulp-group-css-media-queries": "^1.2.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-pngquant": "^1.0.12",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^1.2.3",
    "gulp-replace": "^0.6.1",
    "gulp-rigger": "^0.5.8",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-svg-sprite": "^1.4.0",
    "gulp-svgmin": "^1.2.4",
    "gulp-ttf2woff2": "^2.0.2",
    "gulp-uglify": "^3.0.0",
    "gulp.spritesmith": "^6.9.0",
    "rimraf": "^2.6.2",
    "smart-grid": "^2.0.1"
  }
}
